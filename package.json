{
  "name": "pandoc-grid-table-formatter",
  "displayName": "pandoc-grid-table-formatter",
  "description": "Utility to format pandoc table with grid layout",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pandoc-grid-table-formatter.formatTable",
        "title": "Pandoc-table: format"
      }
  ],
  "menus": {
    "editor/context": [
      {
        "command": "pandoc-grid-table-formatter.formatTable",
        "when": "editorHasSelection",
        "group": "navigation"
      }
    ]
  }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.23.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
